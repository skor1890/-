python
def is_palindrome(string):
    # Удаляем пробелы из строки
    string = string.replace(" ", "")
    # Приводим строку к нижнему регистру
    string = string.lower()
    # Проверяем, является ли строка палиндромом
    if string == string[::-1]:
        return True
    else:
        return False
